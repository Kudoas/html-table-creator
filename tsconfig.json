{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "exclude": ["node_modules", "*.js", "dist"]
}
